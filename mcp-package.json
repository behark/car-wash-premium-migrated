    "ts-node": "^10.9.0"
  },
  "bin": {
    "car-wash-mcp": "./dist/mcp-server.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "car-wash",
    "booking-system",
    "claude"
  ]
}