{
  "name": "premiumautopesu",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:docker": "prisma generate && NODE_ENV=production next build",
    "build:render": "prisma generate && next build --no-lint",
    "postinstall": "prisma generate",
    "start": "node server.js",
    "start:production": "prisma migrate deploy && npm run db:seed:prod && node server.js",
    "deploy:netlify": "bash ./scripts/netlify-deploy.sh 'car-wash-kiilto-loisto'",
    "deploy:netlify:domain": "bash ./scripts/netlify-deploy.sh 'car-wash-kiilto-loisto' www.kiiltoloisto.fi",
    "verify:deploy": "bash ./scripts/verify-deploy.sh",
    "setup:production": "curl -X POST https://your-site.netlify.app/api/setup",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:seed": "ts-node --esm prisma/seed.ts",
    "db:seed:prod": "prisma db seed",
    "prisma:reset": "prisma migrate reset --force",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false --verbose",
    "test:unit": "jest src/**/__tests__/**/*.test.{ts,tsx}",
    "test:integration": "jest __tests__/integration/**/*.test.{ts,tsx}",
    "test:components": "jest src/components/**/__tests__/**/*.test.{ts,tsx}",
    "test:api": "jest src/app/api/**/__tests__/**/*.test.{ts,tsx}",
    "test:e2e": "echo 'E2E tests with Playwright coming soon'",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:silent": "jest --silent",
    "test:verbose": "jest --verbose",
    "test:bail": "jest --bail",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:booking-flow": "jest __tests__/integration/booking-flow.test.tsx",
    "test:real-time": "jest src/components/__tests__/RealTimeBooking.test.tsx",
    "test:services": "jest src/components/__tests__/ServicesGrid.test.tsx",
    "test:health": "jest src/app/api/health/__tests__/route.test.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "mcp:build": "npx tsc mcp-server.ts --outDir dist --target es2020 --module esnext --moduleResolution node",
    "mcp:test": "node test-mcp.js",
    "mcp:test:interactive": "node test-mcp.js --interactive",
    "mcp:start": "node dist/mcp-server.js"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "@prisma/client": "^5.10.0",
    "@sendgrid/mail": "^7.4.0",
    "@sentry/nextjs": "^10.19.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.18",
    "@types/bcrypt": "^5.0.2",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^18.0.0",
    "@types/nodemailer": "^7.0.2",
    "@types/qrcode": "^1.5.5",
    "@types/react": "18.3.25",
    "@types/web-push": "^3.6.3",
    "autoprefixer": "^10.4.0",
    "bcrypt": "^5.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.0.0",
    "idb": "^8.0.0",
    "ioredis": "^5.8.0",
    "lucide-react": "^0.545.0",
    "micro": "^10.0.1",
    "next": "14.2.0",
    "next-auth": "^4.21.0",
    "next-pwa": "^5.6.0",
    "nodemailer": "^6.10.1",
    "postcss": "^8.4.0",
    "prisma": "^5.10.0",
    "qrcode": "^1.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-qr-scanner": "^1.0.0-alpha.11",
    "react-webcam": "^7.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^19.0.0",
    "swiper": "^11.0.0",
    "tailwindcss": "^3.0.0",
    "twilio": "^4.19.0",
    "typescript": "^5.0.0",
    "web-push": "^3.6.0",
    "web-vitals": "^5.1.0",
    "workbox-webpack-plugin": "^7.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@netlify/functions": "^4.2.6",
    "@netlify/plugin-nextjs": "^5.13.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "prettier": "^3.0.0",
    "supertest": "^6.3.0",
    "ts-node": "^10.0.0"
  }
}
