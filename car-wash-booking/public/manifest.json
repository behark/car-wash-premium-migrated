  "shortcuts": [
    {
      "name": "Varaa aika",
      "short_name": "Varaa",
      "description": "Varaa autopesu aika nopeasti",
      "url": "/booking",
      "icons": [
        {
          "src": "/icons/shortcut-booking.svg",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Palvelut",
      "short_name": "Palvelut",
      "description": "Katso kaikki autopesu palvelut",
      "url": "/services",
      "icons": [
        {
          "src": "/icons/shortcut-services.svg",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Yhteystiedot",
      "short_name": "Ota yhteyttä",
      "description": "Ota yhteyttä autopesuun",
      "url": "/contact",
      "icons": [
        {
          "src": "/icons/shortcut-contact.svg",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Galleria",
      "short_name": "Galleria",
      "description": "Katso ennen ja jälkeen kuvia",
      "url": "/gallery",
      "icons": [
        {
          "src": "/icons/shortcut-gallery.svg",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 480
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+carwash",
      "url": "/booking?service=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/booking",
      "accept": {
        "text/calendar": [".ics"]
      },
      "launch_type": "single-client"
    }
  ],
  "share_target": {
    "action": "/booking",
    "method": "GET",
    "params": {
      "title": "service",
      "text": "notes",
      "url": "referrer"
    }
  },
  "screenshots": [
    {
      "src": "/screenshots/desktop-booking.png",
      "sizes": "1280x720",
      "type": "image/png",
